
package gamingcomputerbuild;

public class Laptop extends GamingComputer{
    private int weight;

    public Laptop() {
    }//ends Laptop empty constructor

    public Laptop(String processor, String graphicsCard, int randomAccessMemory, int storage, int weight) {
        super(processor, graphicsCard, randomAccessMemory, storage);
        this.weight = weight;
    }//ends Laptop constructor

    public int getWeight() {
        return weight;
    }//ends getWeight

    public void setWeight(int weight) {
        this.weight = weight;
    }//ends setWeight
    
    
}//ends Laptop class
